<property name="PA Default" active="true">

        <description><![CDATA[
            230 DPI Phone UI<br/>
            200 DPI lockscreen<br/>
            220 DPI launcher<br/>
            220 DPI for all apps<br/>
            6 Holo toggles
        ]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="230"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="360"/>
        <setting type="Hybrid" target="%system_default_layout" value="0"/>
        <setting type="Hybrid" target="%system_default_dpi" value="220"/>
        <setting type="Hybrid" target="%user_default_dpi" value="220"/>
        <setting type="Hybrid" target="android.dpi" value="200"/>
        <setting type="Hybrid" target="com.cyanogenmod.trebuchet.dpi" value="220"/>
        <setting type="Hybrid" target="com.cyanogenmod.trebuchet.layout" value="360"/>
        <setting type="Android" target="status_bar_clock" value="1"/>
        <setting type="Android" target="max_notification_icons" value="5"/>
        <setting type="Android" target="statusbar_toggles_enable" value="1"/>
        <setting type="Android" target="statusbar_toggles" value="ROTATE|WIFI|GPS|BLUETOOTH|SYNC|SILENT"/>
        <setting type="Android" target="statusbar_toggles_show_brightness" value="1"/>
        <setting type="Android" target="statusbar_toggles_disable_scroll" value="1"/>
        <setting type="Android" target="statusbar_toggles_style" value="2"/>

</property>
