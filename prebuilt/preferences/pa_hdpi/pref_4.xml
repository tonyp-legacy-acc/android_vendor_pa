<property name="tonyp - DPI only" active="true" image="/system/etc/paranoid/preferences/images/tonyp_dpionly.jpg">>

        <description><![CDATA[
            Phone workspace<br\>
            190 DPI<br\>
            no layout changes
        ]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="190"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="360"/>
        <setting type="Hybrid" target="%system_default_layout" value="0"/>
        <setting type="Hybrid" target="%system_default_dpi" value="190"/>
        <setting type="Hybrid" target="%user_default_dpi" value="190"/>
        <setting type="Hybrid" target="android.dpi" value="190"/>
        <setting type="Hybrid" target="com.sonyericsson.home.dpi" value="240"/>
        <setting type="Hybrid" target="com.sonyericsson.home.layout" value="360"/>
        <setting type="Hybrid" target="com.android.vending.dpi" value="240"/>
        <setting type="Hybrid" target="com.android.vending.layout" value="360"/>
        <setting type="Android" target="max_notification_icons" value="5"/>
</property>
