<property name="Tablet UI" active="true" image="/system/etc/paranoid/preferences/images/tablet.jpg">

        <description><![CDATA[
            Tablet workspace<br\>
            160 DPI<br\>
            5 Icons<br\>
            No softkeys]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="720"/>
        <setting type="Android" target="max_notification_icons" value="5"/>
        <setting type="Android" target="nav_bar_status" value="0"/>
        <setting type="Hybrid" target="com.paranoid.preferences.dpi" value="160"/>
        <setting type="Hybrid" target="com.paranoid.preferences.layout" value="720"/>
        <setting type="Hybrid" target="com.android.settings.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.settings.layout" value="720"/>
        <setting type="Hybrid" target="com.android.browser.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.browser.layout" value="720"/>
        <setting type="Hybrid" target="com.google.android.gm.dpi" value="160"/>
        <setting type="Hybrid" target="com.google.android.gm.layout" value="720"/>
        <setting type="Hybrid" target="com.android.vending.dpi" value="240"/>
        <setting type="Hybrid" target="com.android.vending.layout" value="360"/>
        <setting type="Hybrid" target="com.sonyericsson.home.dpi" value="240"/>
        <setting type="Hybrid" target="com.sonyericsson.home.layout" value="360"/>

</property>
