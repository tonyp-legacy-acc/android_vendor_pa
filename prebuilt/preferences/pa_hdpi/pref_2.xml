<property name="tonyp's profile" active="true" image="/system/etc/paranoid/preferences/images/tonyp.jpg">

        <description><![CDATA[
            190 overall DPI<br/>
            Phone UI<br/>
            some TabletUI apps<br/>
            6 Holo toggles
        ]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="190"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="360"/>
        <setting type="Hybrid" target="%system_default_layout" value="0"/>
        <setting type="Hybrid" target="%system_default_dpi" value="190"/>
        <setting type="Hybrid" target="%user_default_dpi" value="190"/>
        <setting type="Hybrid" target="android.dpi" value="190"/>
        <setting type="Hybrid" target="com.paranoid.preferences.dpi" value="160"/>
        <setting type="Hybrid" target="com.paranoid.preferences.layout" value="600"/>
        <setting type="Hybrid" target="com.android.settings.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.settings.layout" value="720"/>
        <setting type="Hybrid" target="com.android.browser.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.browser.layout" value="600"/>
        <setting type="Hybrid" target="com.google.android.gm.dpi" value="160"/>
        <setting type="Hybrid" target="com.google.android.gm.layout" value="720"/>
        <setting type="Hybrid" target="com.android.vending.dpi" value="240"/>
        <setting type="Hybrid" target="com.android.vending.layout" value="360"/>
        <setting type="Android" target="status_bar_clock" value="1"/>
        <setting type="Android" target="max_notification_icons" value="5"/>
        <setting type="Android" target="statusbar_toggles_enable" value="1"/>
        <setting type="Android" target="statusbar_toggles" value="WIFI|DATA|2G|ROTATE|TORCH|AIRPLANE_MODE"/>
        <setting type="Android" target="statusbar_toggles_show_brightness" value="1"/>
        <setting type="Android" target="statusbar_toggles_disable_scroll" value="1"/>
        <setting type="Android" target="statusbar_toggles_style" value="2"/>

</property>
